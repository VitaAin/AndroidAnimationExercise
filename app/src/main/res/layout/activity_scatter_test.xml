<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.vita.animation.value.ScatterTestActivity">

    <ImageView
        android:id="@+id/iv_scatter_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/ic_lens_24dp"/>

    <ImageView
        android:id="@+id/iv_scatter_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/ic_lens_24dp"/>

    <ImageView
        android:id="@+id/iv_scatter_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/ic_lens_24dp"/>

    <ImageView
        android:id="@+id/iv_scatter_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/ic_lens_24dp"/>

    <ImageView
        android:id="@+id/iv_scatter_center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/ic_do_not_disturb_on_24dp"/>

</FrameLayout>
